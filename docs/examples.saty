@require: stdjareport
@require: math

@import: ../src/arrows
@import: ../src/commands

open ArrowCommands

in

document (|
  title = {Arrow examples};
  author = {Taichi Uemura};
|) '<
  +math(${
    a \draw!(0.25)!(1.)!(Arrow.line 0.05) b
  });
  +math(${
    a \draw!(0.25)!(1.)!(Arrow.solid (|
      line-width = 0.05;
      head-width = 0.15;
      head-ext = 0.07;
    |)) b
  });
>

